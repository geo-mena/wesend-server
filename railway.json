{
    "services": [
        {
            "name": "send",
            "env": "docker",
            "dockerfilePath": "./Dockerfile",
            "buildCommand": "docker build -t laravel-app .",
            "startCommand": "docker run -p $PORT:8000 laravel-app"
        },
        {
            "name": "scheduler",
            "env": "docker",
            "dockerfilePath": "./Dockerfile",
            "buildCommand": "docker build -t laravel-app .",
            "startCommand": "docker run laravel-app php artisan schedule:work"
        }
    ]
}
